{
  "name": "ens-metadata-tools",
  "version": "1.0.0",
  "description": "Comprehensive toolkit for ENS contract metadata management, security analysis, and subdomain planning",
  "main": "index.mjs",
  "type": "module",
  "bin": {
    "ens-metadata": "./bin/cli.mjs",
    "ens-prober": "./prober/probe-multicall.js",
    "ens-validator": "./bin/naming-validator.js"
  },
  "scripts": {
    "start": "node index.mjs",
    "metadata": "node bin/metadata-generator.js",
    "validate": "node bin/naming-validator.js",
    "probe": "node prober/probe-multicall.js",
    "lookup": "node prober/lookup-resolver-names.js",
    "plan": "node bin/subdomain-planner.mjs --interactive",
    "fill": "node bin/metadata-filler.mjs",
    "security": "node bin/security-analyzer.js",
    "test": "node test-metadata.json",
    "test-proxy": "node test-proxy-system.js",
    "test-real": "node test-real-zero-proxy.js",
    "demo": "node demo-zero-proxy.js",
    "example": "node examples/test-ens-proxy.js"
  },
  "keywords": [
    "ens",
    "ethereum",
    "metadata",
    "security",
    "subdomain",
    "blockchain",
    "web3"
  ],
  "author": "ENS Contracts Team",
  "license": "MIT",
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^4.0.1",
    "@nomicfoundation/hardhat-network-helpers": "^3.0.0",
    "@types/chai": "^5.2.2",
    "chai": "^6.2.0",
    "chalk": "^5.0.0",
    "commander": "^10.0.0",
    "ethers": "^5.7.2",
    "inquirer": "^9.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "hardhat": "^3.0.6",
    "nodemon": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ens-contracts/metadata-tools.git"
  },
  "bugs": {
    "url": "https://github.com/ens-contracts/metadata-tools/issues"
  },
  "homepage": "https://github.com/ens-contracts/metadata-tools#readme"
}
