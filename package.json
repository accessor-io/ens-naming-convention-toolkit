{
  "name": "ens-metadata-tools",
  "version": "1.0.0",
  "description": "Comprehensive toolkit for ENS contract metadata management, security analysis, and subdomain planning",
  "main": "index.mjs",
  "type": "module",
  "bin": {
    "ens-metadata": "./bin/cli.mjs",
    "ens-validator": "./bin/naming-validator.js",
    "ens-cache-browser": "./bin/cache-browser.mjs",
    "evmd": "./bin/evmd.js"
  },
  "scripts": {
    "start": "node index.mjs",
    "metadata": "node bin/metadata-generator.mjs",
    "validate": "node bin/naming-validator.js",
    "validate:metadata": "node bin/contract-naming-cli.js validate --dir data/metadata --schema data/metadata/schema.json || exit 1",
    "lookup": "node prober/lookup-resolver-names.js",
    "plan": "node bin/subdomain-planner.mjs --interactive",
    "fill": "node bin/metadata-filler.mjs",
    "security": "node bin/security-analyzer.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:watch": "jest --watch",
    "test-proxy": "node test-proxy-system.js",
    "test-real": "node test-real-zero-proxy.js",
    "demo": "node demo-zero-proxy.js",
    "example": "node examples/test-ens-proxy.js",
    "cache": "node bin/cache-browser.mjs",
    "typecheck": "tsc -p ."
  },
  "keywords": [
    "ens",
    "ethereum",
    "metadata",
    "security",
    "subdomain",
    "blockchain",
    "web3"
  ],
  "author": "ENS Contracts Team",
  "license": "MIT",
  "dependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.0",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.0",
    "@types/chai": "^5.2.2",
    "chai": "^6.2.0",
    "chalk": "^5.0.0",
    "commander": "^12.0.0",
    "ethers": "^5.7.2",
    "inquirer": "^12.0.0",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^6.1.0",
    "hardhat": "^3.0.6",
    "nodemon": "^3.0.0",
    "typescript": "^5.4.0",
    "@types/node": "^18.19.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.0.0",
    "babel-jest": "^29.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ens-contracts/metadata-tools.git"
  },
  "bugs": {
    "url": "https://github.com/ens-contracts/metadata-tools/issues"
  },
  "homepage": "https://github.com/ens-contracts/metadata-tools#readme"
}
